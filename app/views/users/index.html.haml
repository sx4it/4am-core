
%h2 Listing users

= form_tag users_path, :method => 'get', :id => "users_search" do
  %p
    = text_field_tag :search, params[:search]
    = submit_tag "Search", :name => nil, class: "btn btn-success"
  #users
    = render 'users'

%br

= link_to 'New User', new_user_path, class: "btn btn-success"
