= form_for @key do |f|
  -if @key.errors.any?
    #error_explanation
      %h2= "#{pluralize(@key.errors.count, "error")} prohibited this key from being saved:"
      %ul
        - @key.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :value
    = f.text_field :value
  .field
    = f.label :user
    = f.text_field :user
  .actions
    = f.submit 'Save'
